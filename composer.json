{
    "name": "desmart/laravel-money",
    "type": "library",
    "description": "Manage money in Laravel like a breeze",
    "license": "MIT",
    "keywords": ["desmart", "laravel", "money", "cast"],
    "authors": [
        {
            "name": "Micha≈Ç Golon",
            "email": "gollum@desmart.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/database": "^8.12",
        "moneyphp/money": "^3.3"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.3.3"
    },
    "autoload": {
        "psr-4": {
            "DeSmart\\Larvel\\Money\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DeSmart\\Larvel\\Money\\Tests\\": "tests/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
